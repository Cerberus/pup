{
  "name": "pup",
  "version": "1.0.0",
  "description": "",
  "main": "none",
  "dependencies": {
    "p-limit": "^2.1.0",
    "puppeteer": "^1.11.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/glob": "^7.1.1",
    "@types/puppeteer": "^1.11.1",
    "better-npm-run": "^0.1.1",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prescript": "^0.5555.55",
    "prettier": "^1.15.3",
    "source-map-support": "^0.5.10",
    "standard-version": "^5.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "ttypescript": "^1.5.6",
    "typescript": "^3.2.2",
    "typescript-transform-paths": "^1.0.0"
  },
  "scripts": {
    "start": "better-npm-run prescript",
    "dev": "better-npm-run prescript -d",
    "test:all": "better-npm-run test:all",
    "test:report": "better-npm-run test:report",
    "build:watch": "rm -rf dist; ttsc -w -sourcemap",
    "tslint": "tslint -p .",
    "release": "standard-version"
  },
  "betterScripts": {
    "prescript": {
      "command": "prescript",
      "env": {
        "NODE_PATH": "src"
      }
    },
    "test:all": {
      "command": "node ./dist/testAll.js"
    },
    "test:report": {
      "command": "better-npm-run test:all",
      "env": {
        "ALLURE_RESULTS_DIR": "allure-results"
      }
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint -p . --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "Thanaphol",
  "license": "MIT"
}
